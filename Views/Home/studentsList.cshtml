@*******************************STRONGLY TYPED VIEW**************************@

@model List<practice.Models.StdRecord> 

@{
    ViewBag.Title = "studentsList";
}

<h2>students List</h2>

<table border="5">
    <tr>
        <td><b>ID</b></td>
        <td><b>Name</b></td>
        <td><b>Father Name</b></td>
        <td><b>Roll Number</b></td>
        <td><b>Session</b></td>
        <td><b>Phone</b></td>
        <td><b>CNIC</b></td>
        <td><b>Email</b></td>
    </tr>

    @foreach (var s in Model)
    {
        <tr>
            <td>@s.Id</td>
            <td>@s.Name</td>
            <td>@s.FName</td>
            <td>@s.RollNumber</td>
            <td>@s.Session</td>
            <td>@s.Phone</td>
            <td>@s.CNIC</td>
            <td>@s.Email</td>

            <td>
                <a href="/Home/update/@s.Id">Update</a>
                <a href="/Home/deleteStudent/@s.Id">Delete</a>

            </td>
        </tr>
    }

</table>

